{
  "name": "WiSaw.serverless",
  "version": "1.0.0",
  "description": "WiSaw api as a service hosted on aws lambda",
  "main": "./app.js",
  "scripts": {
    "deploy": "./node_modules/serverless/bin/serverless deploy --stage prod --region us-east-1 -v",
    "remove": "./node_modules/serverless/bin/serverless remove --stage prod --region us-east-1 -v",
    "info": "./node_modules/serverless/bin/serverless info --stage prod --region us-east-1 -v",
    "logs": "./node_modules/serverless/bin/serverless logs -f passwordStrength -t --stage prod --region us-east-1",
    "test": "./node_modules/mocha/bin/mocha --require babel-core/register ./tests/**/*.js --timeout 30000 || exit 0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/echowaves/WiSaw.serverless.git"
  },
  "keywords": [
    "WiSaw",
    "lambda"
  ],
  "author": "echowaves",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/echowaves/WiSaw.serverless/issues"
  },
  "homepage": "https://wisaw.com",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-runtime": "^6.26.0",
    "moment": "^2.19.3",
    "pg": "^7.4.0",
    "sequelize": "^4.15.1",
    "sequelize-cli": "^3.0.0",
    "sharp": "^0.18.4",
    "uuid": "^3.1.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "serverless": "^1.25.0",
    "serverless-webpack": "^4.2.0",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "mocha": "^4.0.1"
  }
}
