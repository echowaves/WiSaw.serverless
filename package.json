{
  "name": "WiSaw.serverless",
  "version": "1.0.0",
  "description": "WiSaw api as a service hosted on aws lambda",
  "main": "./app.js",
  "scripts": {
    "deploy": "./node_modules/serverless/bin/serverless deploy --region us-east-1 -v --stage",
    "info": "./node_modules/serverless/bin/serverless info --region us-east-1 -v --stage",
    "log": "./node_modules/serverless/bin/serverless logs -f abuseReportPost -t --region us-east-1 --stage",
    "test": "./node_modules/nyc/bin/nyc.js ./node_modules/mocha/bin/mocha --require babel-core/register ./api/tests/**/*.js --timeout 30000 || exit 0",
    "eslint": "./node_modules/eslint/bin/eslint.js ./api/** ./migrations/** ./config/**",
    "migrate": "./node_modules/.bin/sequelize db:migrate --env",
    "migrate-undo": "./node_modules/.bin/sequelize db:migrate:undo --env"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/echowaves/WiSaw.serverless.git"
  },
  "keywords": [
    "WiSaw",
    "lambda"
  ],
  "author": "echowaves",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/echowaves/WiSaw.serverless/issues"
  },
  "homepage": "https://wisaw.com",
  "dependencies": {
    "aws-sdk": "^2.181.0",
    "axios": "^0.17.1",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "moment": "^2.19.3",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "read-yaml": "^1.1.0",
    "sequelize": "^4.29.0",
    "sequelize-cli": "^3.2.0",
    "supertest": "^3.0.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "mocha": "^4.0.1",
    "nyc": "^11.4.1",
    "serverless": "^1.25.0",
    "serverless-webpack": "^4.2.0",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  }
}
