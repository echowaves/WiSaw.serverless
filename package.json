{
  "name": "WiSaw.serverless",
  "version": "1.0.0",
  "description": "WiSaw api as a service hosted on aws lambda",
  "main": "./app.js",
  "scripts": {
    "deploy": "./node_modules/serverless/bin/serverless deploy --region us-east-1 -v --stage",
    "info": "./node_modules/serverless/bin/serverless info --region us-east-1 -v --stage",
    "logs": "./node_modules/serverless/bin/serverless logs -f abuseReportCreate -t --region us-east-1 --stage",
    "test": "./node_modules/mocha/bin/mocha --require babel-core/register ./api/tests/**/*.js --timeout 30000 || exit 0",
    "coverage": "NODE_ENV=dev istanbul cover _mocha â€”- -R spec",
    "migrate": "./node_modules/.bin/sequelize $@"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/echowaves/WiSaw.serverless.git"
  },
  "keywords": [
    "WiSaw",
    "lambda"
  ],
  "author": "echowaves",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/echowaves/WiSaw.serverless/issues"
  },
  "homepage": "https://wisaw.com",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-runtime": "^6.26.0",
    "moment": "^2.19.3",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "read-yaml": "^1.1.0",
    "sequelize": "^4.28.6",
    "sharp": "^0.18.4",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.1",
    "serverless": "^1.25.0",
    "serverless-webpack": "^4.2.0",
    "supertest": "^3.0.0",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  }
}
